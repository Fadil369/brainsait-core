{
  "name": "business-in-a-box",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "server:dev": "tsx --tsconfig tsconfig.server.json server/index.ts",
    "server:build": "tsc -p tsconfig.server.json",
    "server:start": "node dist-server/server/index.js",
    "server": "npm run server:build && npm run server:start",
    "test:e2e": "playwright test",
    "compose:up": "docker compose up --build"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.14.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.0",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
